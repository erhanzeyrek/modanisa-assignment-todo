{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "webpack-dev-server --config ./node_modules/@vue/cli-service/webpack.config.js --mode development",
    "test:cypress": "start-server-and-test start http://localhost:8080 cypress",
    "test:pact": "jest  --runInBand --setupFiles ./pact/pactConfig.js --setupFilesAfterEnv ./pact/jest-wrapper.js --testMatch \"**/*.test.pact.js\"",
    "cypress": "cypress run"
  },
  "dependencies": {
    "@pact-foundation/pact-node": "^10.17.1",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@cypress/vue": "^2.2.3",
    "@cypress/webpack-dev-server": "^1.8.0",
    "@pact-foundation/pact": "^9.17.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/server-test-utils": "^1.3.0",
    "@vue/test-utils": "^1.3.0",
    "babel-eslint": "^10.1.0",
    "cypress": "^9.1.1",
    "eslint": "^6.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-plugin": "^4.5.2",
    "jest": "^27.4.5",
    "jest-watch-typeahead": "^1.0.0",
    "start-server-and-test": "^1.14.0",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^4.9.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "plugin:cypress/recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "testEnvironment": "node"
  }
}
