{
  "consumer": {
    "name": "todo-consumer"
  },
  "provider": {
    "name": "todo-provider"
  },
  "interactions": [
    {
      "description": "a GET request to get all todos",
      "providerState": "gets all todos",
      "request": {
        "method": "GET",
        "path": "/todos",
        "headers": {
          "Accept": "application/json; charset=utf-8"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [
          {
            "id": 33,
            "message": "buy some milk"
          }
        ]
      }
    },
    {
      "description": "a POST request to create a todo",
      "providerState": "creates a todo",
      "request": {
        "method": "POST",
        "path": "/addTodo",
        "headers": {
          "Accept": "application/json; charset=utf-8"
        },
        "body": {
          "message": "buy some milk"
        }
      },
      "response": {
        "status": 201,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "id": 44
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}